ns some_ns;

use sf tuple_op (calculate_string_length, splice_string, calculate_statistics);

fn main(input_string: String) -> (String, f64) {
    let old_len = calculate_string_length(input_string);
    let spliced_str = splice_string(input_string);
    let new_len = calculate_string_length(spliced_str);

    let tup = (old_len, spliced_str, new_len);

    let stats = calculate_statistics(tup);

    (spliced_str, stats)
}
