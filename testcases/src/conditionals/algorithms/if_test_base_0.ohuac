ns some_ns;

use sf conditionals::iftest::{modify_string_positive, modify_string_negative, get_cond_result, get_input};

fn main() -> String {
    let cond = get_cond_result();
    let inp = get_input();

    if (cond) {
        modify_string_positive(inp)
    } else {
        modify_string_negative(inp)
    }
}
