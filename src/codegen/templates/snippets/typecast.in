
impl From<Box<GenericType>> for Box<{target_type}> {
    fn from(arg: Box<GenericType>) -> Self {
        unsafe { Box::from_raw(Box::into_raw(arg) as *mut {target_type}) }
    }
}

impl From<Box<{target_type}>> for Box<GenericType> {
    fn from(arg: Box<{target_type}>) -> Self {
        unsafe { Box::from_raw(Box::into_raw(arg) as *mut GenericType) }
    }
}
